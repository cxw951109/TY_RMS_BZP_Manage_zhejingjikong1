                $("#acceptance").append(tr);
            }
        });
    }

    function wantPurchaseList() {
        $.get(wantPurchaseList_url, function (obj) {
            let data_list = obj.data;
            // console.log(data_list)
            $(".WantPurchaseList_tr").remove();
            for (let i = 0; i < data_list.length; i++) {
                let data = data_list[i];
                let status = data.status ? '已申购' : '未处理';
                let tr = '<tr class="normalTableTr table-dashboard WantPurchaseList_tr" style="background-color: rgba(255, 255, 255, 0.05);"><td rowspan="1" colspan="1" class="table-td-undefined text-left" style="border-color: rgb(51, 65, 107); font-size: 16px; padding: 8px 12px; text-align: center;">' + data.name + '</td> <td rowspan="1" colspan="1" class="table-td-undefined text-right" style="border-color: rgb(51, 65, 107); font-size: 16px; padding: 8px 12px; text-align: center;">' + data.stockCount + '</td><td rowspan="1" colspan="1" class="table-td-undefined text-right" style="border-color: rgb(51, 65, 107); font-size: 16px; padding: 8px 12px; text-align: center;">' + data.warningValue + '</td> <td rowspan="1" colspan="1" class="table-td-undefined text-right" style="border-color: rgb(51, 65, 107); font-size: 16px; padding: 8px 12px; text-align: center;">' + status + '</td></tr>'
                $("#WantPurchaseList").append(tr);
            }
        });
    }

    function statisticalData() {
        $.get(statisticalData_url, function (obj) {
            $("#putInCount").html(obj.putInCount);
            $("#useCount").html(obj.useCount);
            $("#returnCount").html(obj.returnCount);
            $("#purchaseCount").html(obj.purchaseCount);
        });
    }

    function stockCount() {
        // 库存量 危化品占比
        $.get(stockCount_url, function (obj) {
            console.log(obj);
            $(".count_stock").remove();
            let count_str = String(obj.allCount);
            for (let num = 0; num < count_str.length; num++) {
                let data = count_str.charAt(num)
