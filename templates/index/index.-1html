                let div = '<div class="flip-single-number count_stock" style="font-family: &quot;Microsoft Yahei&quot;, Arial, sans-serif; font-size: 56px; font-weight: bold; color: rgb(255, 255, 255); margin-left: 8.4px; margin-right: 8.4px; padding: 0px 0.1em; box-sizing: content-box; width: 0.6em; text-align: center; background-color: rgb(15, 57, 107);">' + data + '</div>'
                $('#allStock').append(div);
            }
            let hazardousProportion = obj.hazardousProportion * 100;
            hazardousProportion = hazardousProportion.toFixed(2);
            let proportion = hazardousProportion + '%'
            $("#hazardousProportion").html(proportion);
            $("#hazardousProportionCss").css("width", proportion);
            let SpotCss = "calc(" + proportion + " - 5px)"
            $("#hazardousProportionSpotCss").css("left", SpotCss);

