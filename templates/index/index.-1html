    // 实时显示时间
    function showTime() {
        var today = new Date;
        var y = today.getFullYear();
        var M = today.getMonth() + 1;
        var d = today.getDate();
        var h = today.getHours();
        var m = today.getMinutes();
        var s = today.getSeconds();
        if (M < 10) {
            M = '0' + M
        }
        if (d < 10) {
            d = '0' + d
        }
        if (h < 10) {
            h = '0' + h
        }
        if (m < 10) {
            m = '0' + m
        }
        if (s < 10) {
            s = '0' + s
        }
        $(".time-chart-container").html(y + '-' + M + '-' + d + '  ' + h + ':' + m + ':' + s);
    }
    function warningListJson() {
